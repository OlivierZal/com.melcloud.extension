{
  "compilerOptions": {
    "lib": ["DOM", "ESNext"],
    "module": "NodeNext",
    "noImplicitOverride": true,
    "noUncheckedSideEffectImports": true,
    "outDir": ".homeybuild",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES2022"
  }
}
